{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "orderflowclase": {
      "error": "This resource does not support generation in the manifest."
    },
    "postgres": {
      "type": "container.v1",
      "connectionString": "Host={postgres.bindings.tcp.host};Port={postgres.bindings.tcp.port};Username=postgres;Password={postgres-password.value}",
      "image": "docker.io/library/postgres:17.6",
      "volumes": [
        {
          "name": "postgres",
          "target": "/var/lib/postgresql/data",
          "readOnly": false
        }
      ],
      "env": {
        "POSTGRES_HOST_AUTH_METHOD": "scram-sha-256",
        "POSTGRES_INITDB_ARGS": "--auth-host=scram-sha-256 --auth-local=scram-sha-256",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "{postgres-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "port": 5431,
          "targetPort": 5432
        }
      }
    },
    "servertienda": {
      "type": "value.v0",
      "connectionString": "{postgres.connectionString};Database=servertienda"
    },
    "catalogdb": {
      "type": "value.v0",
      "connectionString": "{postgres.connectionString};Database=catalogdb"
    },
    "ordersdb": {
      "type": "value.v0",
      "connectionString": "{postgres.connectionString};Database=ordersdb"
    },
    "redis": {
      "type": "container.v1",
      "connectionString": "{redis.bindings.tcp.host}:{redis.bindings.tcp.port},password={redis-password.value}",
      "image": "docker.io/library/redis:8.2",
      "entrypoint": "/bin/sh",
      "args": [
        "-c",
        "redis-server --requirepass $REDIS_PASSWORD --save 60 1"
      ],
      "volumes": [
        {
          "name": "redis-data-identity",
          "target": "/data",
          "readOnly": false
        }
      ],
      "env": {
        "REDIS_PASSWORD": "{redis-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 6379
        }
      }
    },
    "rabbitmq": {
      "type": "container.v1",
      "connectionString": "amqp://guest:{rabbitmq-password-uri-encoded.value}@{rabbitmq.bindings.tcp.host}:{rabbitmq.bindings.tcp.port}",
      "image": "docker.io/library/rabbitmq:4.2-management",
      "volumes": [
        {
          "name": "rabbitmq-data",
          "target": "/var/lib/rabbitmq",
          "readOnly": false
        }
      ],
      "env": {
        "RABBITMQ_DEFAULT_USER": "guest",
        "RABBITMQ_DEFAULT_PASS": "{rabbitmq-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5672
        },
        "management": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 15672
        }
      }
    },
    "maildev": {
      "type": "container.v1",
      "image": "maildev/maildev:latest",
      "bindings": {
        "web": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 1080
        },
        "smtp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 1025
        }
      }
    },
    "tiendainspireidentity": {
      "type": "project.v1",
      "path": "../TiendaInspireIdentity/TiendaInspireIdentity.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{tiendainspireidentity.bindings.http.targetPort}",
        "ConnectionStrings__rabbitmq": "{rabbitmq.connectionString}",
        "RABBITMQ_HOST": "{rabbitmq.bindings.tcp.host}",
        "RABBITMQ_PORT": "{rabbitmq.bindings.tcp.port}",
        "RABBITMQ_USERNAME": "guest",
        "RABBITMQ_PASSWORD": "{rabbitmq-password.value}",
        "RABBITMQ_URI": "amqp://guest:{rabbitmq-password-uri-encoded.value}@{rabbitmq.bindings.tcp.host}:{rabbitmq.bindings.tcp.port}",
        "ConnectionStrings__servertienda": "{servertienda.connectionString}",
        "SERVERTIENDA_HOST": "{postgres.bindings.tcp.host}",
        "SERVERTIENDA_PORT": "{postgres.bindings.tcp.port}",
        "SERVERTIENDA_USERNAME": "postgres",
        "SERVERTIENDA_PASSWORD": "{postgres-password.value}",
        "SERVERTIENDA_URI": "postgresql://postgres:{postgres-password-uri-encoded.value}@{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/servertienda",
        "SERVERTIENDA_JDBCCONNECTIONSTRING": "jdbc:postgresql://{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/servertienda",
        "SERVERTIENDA_DATABASE": "servertienda",
        "ConnectionStrings__redis": "{redis.connectionString}",
        "REDIS_HOST": "{redis.bindings.tcp.host}",
        "REDIS_PORT": "{redis.bindings.tcp.port}",
        "REDIS_PASSWORD": "{redis-password.value}",
        "REDIS_URI": "redis://:{redis-password-uri-encoded.value}@{redis.bindings.tcp.host}:{redis.bindings.tcp.port}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "tiendainspireidentity"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "tiendainspirecatalog": {
      "type": "project.v1",
      "path": "../TiendaInspire.Catalog/TiendaInspire.Catalog.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{tiendainspirecatalog.bindings.http.targetPort}",
        "ConnectionStrings__catalogdb": "{catalogdb.connectionString}",
        "CATALOGDB_HOST": "{postgres.bindings.tcp.host}",
        "CATALOGDB_PORT": "{postgres.bindings.tcp.port}",
        "CATALOGDB_USERNAME": "postgres",
        "CATALOGDB_PASSWORD": "{postgres-password.value}",
        "CATALOGDB_URI": "postgresql://postgres:{postgres-password-uri-encoded.value}@{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/catalogdb",
        "CATALOGDB_JDBCCONNECTIONSTRING": "jdbc:postgresql://{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/catalogdb",
        "CATALOGDB_DATABASE": "catalogdb",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "tiendainspirecatalog"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "tiendainspireorders": {
      "type": "project.v1",
      "path": "../TiendaInspire.Orders/TiendaInspire.Orders.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{tiendainspireorders.bindings.http.targetPort}",
        "ConnectionStrings__ordersdb": "{ordersdb.connectionString}",
        "ORDERSDB_HOST": "{postgres.bindings.tcp.host}",
        "ORDERSDB_PORT": "{postgres.bindings.tcp.port}",
        "ORDERSDB_USERNAME": "postgres",
        "ORDERSDB_PASSWORD": "{postgres-password.value}",
        "ORDERSDB_URI": "postgresql://postgres:{postgres-password-uri-encoded.value}@{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/ordersdb",
        "ORDERSDB_JDBCCONNECTIONSTRING": "jdbc:postgresql://{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/ordersdb",
        "ORDERSDB_DATABASE": "ordersdb",
        "ConnectionStrings__rabbitmq": "{rabbitmq.connectionString}",
        "RABBITMQ_HOST": "{rabbitmq.bindings.tcp.host}",
        "RABBITMQ_PORT": "{rabbitmq.bindings.tcp.port}",
        "RABBITMQ_USERNAME": "guest",
        "RABBITMQ_PASSWORD": "{rabbitmq-password.value}",
        "RABBITMQ_URI": "amqp://guest:{rabbitmq-password-uri-encoded.value}@{rabbitmq.bindings.tcp.host}:{rabbitmq.bindings.tcp.port}",
        "TIENDAINSPIRECATALOG_HTTP": "{tiendainspirecatalog.bindings.http.url}",
        "services__tiendainspirecatalog__http__0": "{tiendainspirecatalog.bindings.http.url}",
        "TIENDAINSPIRECATALOG_HTTPS": "{tiendainspirecatalog.bindings.https.url}",
        "services__tiendainspirecatalog__https__0": "{tiendainspirecatalog.bindings.https.url}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "tiendainspireorders"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "tiendainspire-apigateway": {
      "type": "project.v1",
      "path": "../TiendaAspire.ApiGateway/TiendaAspire.ApiGateway.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{tiendainspire-apigateway.bindings.http.targetPort}",
        "ConnectionStrings__redis": "{redis.connectionString}",
        "REDIS_HOST": "{redis.bindings.tcp.host}",
        "REDIS_PORT": "{redis.bindings.tcp.port}",
        "REDIS_PASSWORD": "{redis-password.value}",
        "REDIS_URI": "redis://:{redis-password-uri-encoded.value}@{redis.bindings.tcp.host}:{redis.bindings.tcp.port}",
        "TIENDAINSPIREIDENTITY_HTTP": "{tiendainspireidentity.bindings.http.url}",
        "services__tiendainspireidentity__http__0": "{tiendainspireidentity.bindings.http.url}",
        "TIENDAINSPIREIDENTITY_HTTPS": "{tiendainspireidentity.bindings.https.url}",
        "services__tiendainspireidentity__https__0": "{tiendainspireidentity.bindings.https.url}",
        "TIENDAINSPIRECATALOG_HTTP": "{tiendainspirecatalog.bindings.http.url}",
        "services__tiendainspirecatalog__http__0": "{tiendainspirecatalog.bindings.http.url}",
        "TIENDAINSPIRECATALOG_HTTPS": "{tiendainspirecatalog.bindings.https.url}",
        "services__tiendainspirecatalog__https__0": "{tiendainspirecatalog.bindings.https.url}",
        "TIENDAINSPIREORDERS_HTTP": "{tiendainspireorders.bindings.http.url}",
        "services__tiendainspireorders__http__0": "{tiendainspireorders.bindings.http.url}",
        "TIENDAINSPIREORDERS_HTTPS": "{tiendainspireorders.bindings.https.url}",
        "services__tiendainspireorders__https__0": "{tiendainspireorders.bindings.https.url}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "tiendainspire-apigateway"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "tiendinspire-notificaciones": {
      "type": "project.v1",
      "path": "../TiendInspire.Notificaciones/TiendInspire.Notificaciones.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ConnectionStrings__rabbitmq": "{rabbitmq.connectionString}",
        "RABBITMQ_HOST": "{rabbitmq.bindings.tcp.host}",
        "RABBITMQ_PORT": "{rabbitmq.bindings.tcp.port}",
        "RABBITMQ_USERNAME": "guest",
        "RABBITMQ_PASSWORD": "{rabbitmq-password.value}",
        "RABBITMQ_URI": "amqp://guest:{rabbitmq-password-uri-encoded.value}@{rabbitmq.bindings.tcp.host}:{rabbitmq.bindings.tcp.port}",
        "Email__SmtpHost": "{maildev.bindings.smtp.host}",
        "Email__SmtpPort": "{maildev.bindings.smtp.port}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "tiendinspire-notificaciones"
      }
    },
    "postgres-password": {
      "type": "parameter.v0",
      "value": "{postgres-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22
            }
          }
        }
      }
    },
    "postgres-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{postgres-password.value}",
      "filter": "uri"
    },
    "redis-password": {
      "type": "parameter.v0",
      "value": "{redis-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22,
              "special": false
            }
          }
        }
      }
    },
    "redis-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{redis-password.value}",
      "filter": "uri"
    },
    "rabbitmq-password": {
      "type": "parameter.v0",
      "value": "{rabbitmq-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22,
              "special": false
            }
          }
        }
      }
    },
    "rabbitmq-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{rabbitmq-password.value}",
      "filter": "uri"
    },
    "orderflowclase-dashboard": {
      "type": "container.v1",
      "image": "mcr.microsoft.com/dotnet/nightly/aspire-dashboard:latest",
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 18888
        },
        "otlp-grpc": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 18889
        }
      }
    }
  }
}